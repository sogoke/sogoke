<% if current_user.favorite_of?(sogoke) %>
	<%= link_to '取消喜欢', "javascript:void(0);", :id => "unlike", :"data-id" => current_user.favorite(sogoke).id, :"data-token" => sogoke.class.to_s.downcase %>
<% else %>
    <%= link_to '喜欢', "javascript:void(0);", :id => "like", :"data-id" => sogoke.id, :"data-token" => sogoke.class.to_s.downcase %>
<% end -%>